{
  "manifest_version": 2,
  "name": "JusTab",
  "version": "0.2.0",
  "author": "Johan van Eck",
  "description": "Keep track of all your online services. With just a tab you see what's going on!",
  "icons": {
    "16": "/img/app_icons/JusTab-16x16.png",
    "32": "/img/app_icons/JusTab-32x32.png",
    "48": "/img/app_icons/JusTab-48x48.png",
    "128": "/img/app_icons/JusTab-128x128.png",
    "256": "/img/app_icons/JusTab-256x256.png",
    "512": "/img/app_icons/JusTab-512x512.png"
  },
  
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "options_page": "options.html",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "key": "",
  "oauth2": {
    "client_id": "597229797223-em315o6neelklo2k7e9uv3ahtuo54lue.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar",
      "https://mail.google.com/",
      "https://www.googleapis.com/auth/gmail.readonly"
    ]
  },
  "permissions": [
    "identity",
    "identity.email",
    "storage",
    "alarms",
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}